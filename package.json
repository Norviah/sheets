{
  "name": "@norviah/sheets",
  "version": "2.0.0",
  "description": "Convert Google Sheets into JSON.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build:src": "./node_modules/typescript/bin/tsc",
    "build:docs": "/bin/rm -rf ./docs; ./node_modules/typedoc/bin/typedoc --plugin typedoc-plugin-markdown; sed -i '' '4d;5d' ./docs/README.md",
    "publish:patch": "npm run build:src && ./node_modules/standard-version/bin/cli.js --release-as patch",
    "publish:minor": "npm run build:src && ./node_modules/standard-version/bin/cli.js --release-as minor",
    "publish:major": "npm run build:src && ./node_modules/standard-version/bin/cli.js --release-as major",
    "publish:version": "npm run build:src && ./node_modules/standard-version/bin/cli.js --release-as $1",
    "publish": "git push --follow-tags origin master && npm publish",
    "run": "npm run build:src && node ./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/norviah/sheets.git"
  },
  "keywords": [
    "sheets"
  ],
  "author": "norviah",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Norviah/sheets/issues"
  },
  "homepage": "https://github.com/Norviah/sheets#readme",
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/lodash": "^4.14.152",
    "@types/node": "^14.0.5",
    "@types/readline-sync": "^1.4.3",
    "@types/ws": "^7.2.4",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "standard-version": "^8.0.0",
    "typedoc": "^0.17.7",
    "typedoc-plugin-markdown": "^2.2.17",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "app-root-path": "^3.0.0",
    "google-auth-library": "^6.0.0",
    "googleapis": "^51.0.0",
    "lodash": "^4.17.15",
    "ora": "^4.0.4",
    "readline-sync": "^1.4.10"
  }
}
